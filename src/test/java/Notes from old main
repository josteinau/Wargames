        // Creating the horde
        List<units.Unit> orcs = new ArrayList<units.Unit>();
        orcs.add(new units.CavalryUnit("units.Unit",25));
        orcs.add(new units.CavalryUnit("Unit3",25));
        orcs.add(new units.CavalryUnit("Unit3",25));
        orcs.add(new units.RangedUnit("Rango",25));
        orcs.add(new units.InfantryUnit("Pls", 25));
        orcs.add(new units.InfantryUnit("Pls2", 25));
        orcs.add(new units.InfantryUnit("Pls3", 25));
        Battle.Army Horde = new Battle.Army("Orcarmy", orcs);
        System.out.println("Display orc army");
        System.out.println("total hp = 175");
        System.out.println(Horde.getArmyHitpoints(orcs));

        Horde.getAllUnits(orcs);
        System.out.println("Infunits here");
        System.out.println(Horde.getCommanderUnits(orcs));
        System.out.println(Horde.getCavalryUnits(orcs));
        System.out.println(Horde.getInfantryUnits(orcs));
        System.out.println(Horde.getRangedUnits(orcs));

        System.out.println("Random unit: " + Horde.getRandomElement(orcs));
        System.out.println("Checks if has units, expect true:" + Horde.hasUnits(orcs));

